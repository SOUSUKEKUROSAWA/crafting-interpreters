var globalOne;
var globalTwo;

fun main() {
    for (var a = 1; a <= 2; a = a + 1) {
        fun closure() {
            print a;
        }
        if (globalOne == nil) {
            globalOne = closure;
        } else {
            globalTwo = closure;
        }
    }
}

main();
globalOne(); // 3? 1?
globalTwo(); // 3? 2?
